{
  "name": "Acc Fixer",
  "version": "1.5",
  "description": "Chose right twitter account!",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://jbs.zendesk.com/agent/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
