{
  "name": "Zendesk Twitter account fixer",
  "version": "1.0.2",
  "description": "This extension chooses right twitter account instead of default one in a Zendesk app",
  "manifest_version": 2,
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://jbs.zendesk.com/agent/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
